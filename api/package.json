{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -p ./tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "check-format": "prettier --check --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2-runtime start ecosystem.config.js ",
    "schema:drop": "typeorm schema:drop",
    "schema:sync": "typeorm schema:sync",
    "typeorm": "ts-node ../node_modules/.bin/typeorm",
    "seed:run": "ts-node ../node_modules/.bin/typeorm-seeding seed",
    "seed:config": "ts-node ../node_modules/.bin/typeorm-seeding config",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "check-lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:e2e": "nest build -p tsconfig.e2e-test.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.5.1",
    "@nestjs/cli": "^8.2.1",
    "@nestjs/common": "7.6.17",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.17",
    "@nestjs/graphql": "7.10.6",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "7.6.17",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/terminus": "7.1.1",
    "@nestjs/typeorm": "7.1.5",
    "@types/bcryptjs": "2.4.2",
    "@types/cron": "^1.7.2",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "apollo-server-express": "2.24.1",
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "cookie-parser": "1.4.5",
    "crypto": "1.0.1",
    "date-fns": "^2.22.1",
    "ejs": "3.1.6",
    "graphql": "15.5.0",
    "graphql-tools": "7.0.5",
    "graphql-type-json": "0.3.2",
    "graphql-upload": "12.0.0",
    "jwks-rsa": "2.0.3",
    "jwt-decode": "3.1.2",
    "mime-types": "^2.1.31",
    "nest-raven": "7.2.0",
    "nestjs-admin": "0.4.1",
    "nodemailer": "6.6.1",
    "nunjucks": "^3.2.3",
    "passport": "^0.5.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "4.0.0",
    "pg": "8.6.0",
    "pm2": "^5.0.2",
    "ramda": "0.27.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.1.0",
    "typeorm": "0.2.32",
    "typeorm-seeding": "1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@types/axios": "0.14.0",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.11",
    "@types/faker": "5.5.5",
    "@types/jest": "26.0.23",
    "@types/node": "15.6.0",
    "@types/passport-google-oauth20": "2.0.7",
    "@types/passport-jwt": "3.0.5",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "apollo-server-types": "0.8.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "faker": "5.5.3",
    "jest": "26.6.3",
    "prettier": "2.3.0",
    "sqlite3": "5.0.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.6",
    "ts-loader": "8.1.0",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
